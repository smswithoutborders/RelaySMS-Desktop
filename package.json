{
  "name": "relaysms",
  "version": "0.1.0",
  "private": true,
  "license": "GPL-3.0-only",
  "main": "public/electron.js",
  "author": "SMSWithoutBorders <developers@smswithoutborders.com>",
  "description": "Relay SMS Desktop App",
  "homepage": "./",
  "category": "Communication",
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@grpc/grpc-js": "^1.10.8",
    "@grpc/proto-loader": "^0.7.13",
    "@mui/icons-material": "^5.16.0",
    "@mui/material": "^6.1.7",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "assert": "^2.1.0",
    "axios": "^1.7.3",
    "base64-arraybuffer": "^1.0.2",
    "base64url": "^3.0.1",
    "browserify-fs": "^1.0.0",
    "buffer": "^6.0.3",
    "caniuse-lite": "^1.0.30001664",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "electron-icon-builder": "^2.0.1",
    "fernet": "^0.3.2",
    "i18next": "^23.11.4",
    "mui-tel-input": "^5.1.2",
    "prop-types": "^15.8.1",
    "protobufjs": "^7.3.0",
    "react": "^18.3.1",
    "react-app-rewired": "^2.2.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.1",
    "react-icons": "^5.2.1",
    "react-joyride": "^2.8.2",
    "react-phone-number-input": "^3.4.1",
    "react-router-dom": "^6.23.0",
    "react-scripts": "5.0.1",
    "stream-browserify": "^3.0.0",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "utf8": "^3.0.0",
    "util": "^0.12.5",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "preelectron-pack": "yarn build",
    "electron:start": "concurrently -k \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electronmon .\"",
    "rebuild": "electron-rebuild",
    "postinstall": "electron-rebuild",
    "electron:package:mac": "yarn rebuild && yarn build && electron-builder --mac",
    "electron:package:win": "yarn rebuild && yarn build && electron-builder --win",
    "electron:package:linux": "yarn rebuild && yarn build && electron-builder --linux"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@electron-forge/maker-deb": "^7.4.0",
    "@testing-library/dom": "^10.1.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^34.0.0",
    "electron-builder": "^24.13.3",
    "electronmon": "^2.0.3",
    "file-loader": "^6.2.0",
    "https-browserify": "^1.0.0",
    "node-libs-browser": "^2.2.1",
    "stream-http": "^3.2.0",
    "terser-webpack-plugin": "^5.3.10",
    "url": "^0.11.3",
    "wait-on": "^7.2.0"
  },
  "optionalDependencies": {
    "better-sqlite3": "11.8.1",
    "keytar": "^7.9.0"
  },
  "build": {
    "appId": "com.smswithoutborders.relay",
    "productName": "RelaySMS",
    "compression": "store",
    "protocols": [
      {
        "name": "RelaySMS",
        "schemes": [
          "apps"
        ]
      }
    ],
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis",
      "publisherName": "Afkanerd",
      "icon": "public/images"
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "AppImage",
        "snap"
      ],
      "icon": "public/images",
      "category": "Communication"
    },
    "rpm": {
      "compression": "xz"
    },
    "snap": {
      "grade": "stable",
      "confinement": "strict",
      "plugs": [
        "home",
        "network",
        "network-bind"
      ]
    }
  }
}
